mixin link(href, index, text, type, links)
    if type === 'link'
        a(href=href target='_blank' class='link') #{text}
    if type === 'crumb'
        if index < links.length - 1
            a(href=href target='_blank' class='link breadcrumb_link') #{text}
        else
            span.breadcrumb_link #{text}